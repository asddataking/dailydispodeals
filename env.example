# Supabase
# Replace with your actual Supabase project URL from https://app.supabase.com
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Public Supabase vars (for client-side edge function calls)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret
STRIPE_MONTHLY_PRICE_ID=price_your_monthly_price_id
STRIPE_YEARLY_PRICE_ID=price_your_yearly_price_id

# Resend
RESEND_API_KEY=re_your_resend_api_key

# Cron Secrets (generate random strings)
CRON_SECRET=your_random_cron_secret_here
INGESTION_CRON_SECRET=your_random_ingestion_cron_secret_here

# App URL (your Vercel deployment URL)
APP_URL=https://yourdomain.com

# Optional for dev
DEV_SECRET=your_dev_secret_for_seed_route
NODE_ENV=development

# For ingestion pipeline - Vercel AI Gateway
AI_GATEWAY_URL=https://gateway.vercel.ai/v1/chat/completions
AI_GATEWAY_API_KEY=your_vercel_ai_gateway_api_key

# Google APIs
# Used for geocoding, Places, and Maps-related requests
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Used for YouTube Data API (v3)
GOOGLE_YOUTUBE_API_KEY=your_google_youtube_api_key

# ID of the Dank N' Devour YouTube channel (e.g. UCxxxxxxxxxxxx)
YOUTUBE_CHANNEL_ID=your_youtube_channel_id

# Unsubscribe secret for generating secure unsubscribe tokens
UNSUBSCRIBE_SECRET=your_random_unsubscribe_secret_here

# Alternative: If using direct OpenAI (not recommended)
# OPENAI_API_KEY=sk-your_openai_api_key

# Rate Limiting (optional - for production)
# If using Upstash Redis for distributed rate limiting:
# UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
# UPSTASH_REDIS_REST_TOKEN=your-redis-token
# If not configured, falls back to in-memory rate limiting (resets on cold starts)
